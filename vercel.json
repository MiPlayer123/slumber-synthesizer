{
  "rewrites": [
    {
      "source": "/api/functions/v1/:path*",
      "destination": "https://api.lucidrem.com/functions/v1/:path*"
    },
    {
      "source": "/api/og/:path*",
      "destination": "/api/og/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/checkout-complete&success=true",
      "destination": "/checkout-complete?success=true",
      "permanent": false
    },
    {
      "source": "/checkout-complete&canceled=true",
      "destination": "/checkout-complete?canceled=true",
      "permanent": false
    }
  ]
}
